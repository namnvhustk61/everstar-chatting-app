<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="TabBarItem">
        <attr name="title" />
        <attr name="activeIcon" format="reference" />
        <attr name="inactiveIcon" format="reference" />
        <attr name="colorTextActive" format="color" />
        <attr name="colorTextInactive" format="color" />
        <attr name="size_icon" format="dimension" />
        <attr name="size_text" format="dimension" />
        <attr name="size_margin_text_icon" format="dimension" />
    </declare-styleable>

    <declare-styleable name="RadiusLayout">
        <attr name="backgroundColor" />
        <attr name="radius" format="dimension" />
        <attr name="radiusTopLeft" format="dimension" />
        <attr name="radiusTopRight" format="dimension" />
        <attr name="radiusBottomLeft" format="dimension" />
        <attr name="radiusBottomRight" format="dimension" />
        <attr name="strokeWidth" />
        <attr name="strokeColor" />
        <attr name="offStateOnPressed" format="boolean" />
    </declare-styleable>
</resources>